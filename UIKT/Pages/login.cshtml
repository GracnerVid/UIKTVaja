@page
@model UIKT.Pages.loginModel
@{
    ViewData["Title"] = "Log in";
}

<div class="text-center">
    <h4>Log in</h4>
    <form method="post">
        <label for="fname">Uporabniško ime:</label>
        <input type="text" id="fname" name="fname"/><br /><br />
        <label for="fpass">Geslo:</label>
        <input type="password" id="fpass" name="fpass" /><br /><br />
        <input type="submit" id="loginBtn" onclick="loginBtn_Click" value="Prijava" />
    </form>
</div>